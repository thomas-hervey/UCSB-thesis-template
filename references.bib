@article{Spink2002,
abstract = {The Web has become a worldwide source of information and a$\backslash$nmainstream business tool. Are human information needs and searching$\backslash$nbehaviors evolving along with Web content? As part of a body of research$\backslash$nstudying this question, we have analyzed three data sets culled from$\backslash$nmore than one million queries submitted by more than 200,000 users of$\backslash$nthe Excite Web search engine, collected in September 1997, December$\backslash$n1999, and May 2001. This longitudinal benchmark study shows that public$\backslash$nWeb searching is evolving in certain directions. Specifically, search$\backslash$ntopics have shifted from entertainment and sex to commerce and people,$\backslash$nbut there is little change in query lengths or frequency per user.$\backslash$nSearch topics have shifted, but there is little change in user search$\backslash$nbehaviors},
annote = {Reinforcement from the other Spink paper. Also suggests that topics have changed, and there is a bit of change in search habbit (i.e., more search terms, more boolean operators used). Most other things stayed the same (e.g., number of views per query).},
