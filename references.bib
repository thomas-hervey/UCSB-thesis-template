journal = {Proceedings of the 2015 Internet Measurement Conference},
keywords = {all or part of,classroom use is granted,copies are not made,geolocation,internet filter bubble,or,or distributed,or hard copies of,permission to make digital,personalization,search,this work for personal,without fee provided that},
mendeley-groups = {Dissertation/Exams/Writtens/Reading List/To Read/3. Specialization (GIR),Dissertation/Exams/Writtens/combined{\_}writtens,Dissertation/Exams/Orals/related{\_}work},
pages = {121--127},
title = {{Location, Location, Location: The Impact of Geolocation on Web Search Personalization}},
