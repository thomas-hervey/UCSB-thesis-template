file = {:Users/thomashervey/Library/Application Support/Mendeley Desktop/Downloaded/Ballatore et al. - 2016 - Spatial approaches to information search.pdf:pdf;:Users/thomashervey/Downloads/Special issue introduction Spatial approaches to information search.pdf:pdf},
